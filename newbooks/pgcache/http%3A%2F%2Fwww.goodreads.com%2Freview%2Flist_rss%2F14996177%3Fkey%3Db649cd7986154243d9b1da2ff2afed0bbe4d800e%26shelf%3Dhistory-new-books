    <guid><![CDATA[http://www.goodreads.com/review/show/465552013?utm_medium=api&utm_source=rss]]></guid>
    <pubDate><![CDATA[Wed, 28 Nov 2012 10:28:18 -0800]]></pubDate>
    <title>Andrew Marvell: The Chameleon</title>
    <link><![CDATA[http://www.goodreads.com/review/show/465552013?utm_medium=api&utm_source=rss]]></link>
    <book_id>13220918</book_id>
    <book_image_url><![CDATA[http://d.gr-assets.com/books/1340758348s/13220918.jpg]]></book_image_url>
    <book_small_image_url><![CDATA[http://d.gr-assets.com/books/1340758348s/13220918.jpg]]></book_small_image_url>
    <book_medium_image_url><![CDATA[http://d.gr-assets.com/books/1340758348m/13220918.jpg]]></book_medium_image_url>
    <book_large_image_url><![CDATA[http://d.gr-assets.com/books/1340758348l/13220918.jpg]]></book_large_image_url>
